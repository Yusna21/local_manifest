<?xml version="1.0" encoding="UTF-8"?>

<manifest>
  <!-- remote -->
  <remote name="me" fetch="https://github.com/romiyusnandar/"/>
  <remote name="pos" fetch="https://github.com/PixelOS-Devices/" />
  <remote name="los" fetch="https://github.com/LineageOS/" />
  <remote name="pulkit" fetch="https://github.com/Pulkit077/"/>

  <!-- remove packages -->
  <remove-project name="android_vendor_orion" />
  <!-- device tree -->
  <project path="device/xiaomi/sweet" name="android_device_xiaomi_sweet" remote="me" revision="WIP" />
  <!-- vendor tree -->
  <project path="vendor/xiaomi/sweet" name="proprietary_vendor_xiaomi_sweet" remote="me" revision="WIP" />
  <!-- kernel tree -->
  <project path="kernel/xiaomi/sweet" name="kernel_xiaomi_sweet" remote="pulkit" revision="lineage-21" />
  <!-- miui camera -->
  <project path="vendor/xiaomi/sweet-miuicamera" name="vendor_xiaomi_sweet-miuicamera" remote="pos" revision="fourteen" />
  <!-- needed by source -->
  <project path="hardware/xiaomi" name="android_hardware_xiaomi" remote="los" revision="lineage-21" />
  <project path="vendor/lineage" name="android_vendor_orion" remote="me" revision="14.0" />
  <!-- sign build -->
  <project path="vendor/lineage-priv/keys" name="vendor_lineage-priv_keys" remote="me" revision="fourteen" />

</manifest>
